            <para/>
            <ITEXT CH="    echo &quot;&quot; &gt; $FILENAME"/>
            <para/>
            <ITEXT CH="  }"/>
            <para/>
            <para/>
            <ITEXT CH="  case $1 in"/>
            <para/>
            <ITEXT CH="    &quot;New&quot;)"/>
            <para/>
            <ITEXT CH="      $1 $2"/>
            <para/>
            <ITEXT CH="      ;;"/>
            <para/>
            <ITEXT CH="    *)"/>
            <para/>
            <ITEXT CH="      echo &quot;no file $1 or command error&quot;"/>
            <para/>
            <ITEXT CH="      exit"/>
            <para/>
            <ITEXT CH="      ;;"/>
            <para/>
            <ITEXT CH="  esac"/>
            <para/>
            <ITEXT CH="}"/>
            <para/>
            <para/>
            <ITEXT CH="function Size {"/>
            <para/>
            <ITEXT CH="  local local_file=&quot;tmp_$FILENAME&quot;"/>
            <para/>
            <ITEXT CH="  local defalut_grid=&quot;          0&quot;"/>
            <para/>
            <ITEXT CH="  local column_grid=&quot;          0&quot;"/>
            <para/>
            <para/>
            <ITEXT CH="  function Set_Column {"/>
            <para/>
            <ITEXT CH="    function Set_Column_Loop {"/>
            <para/>
            <ITEXT CH="      case &quot;$1&quot; in"/>
            <para/>
            <ITEXT CH="        &quot;1&quot;)"/>
            <para/>
            <ITEXT CH="          return 0"/>
            <para/>
            <ITEXT CH="          ;;"/>
            <para/>
            <ITEXT CH="        *)"/>
            <para/>
            <ITEXT CH="          column_grid=&quot;$column_grid$defalut_grid&quot;"/>
            <para/>
            <ITEXT CH="          Pred &quot;$1&quot;"/>
            <para/>
            <ITEXT CH="          Set_Column_Loop &quot;$RESULT&quot;"/>
            <para/>
            <ITEXT CH="          ;;"/>
            <para/>
            <ITEXT CH="      esac"/>
            <para/>
            <ITEXT CH="    }"/>
            <para/>
            <ITEXT CH="    Set_Column_Loop $1"/>
            <para/>
            <ITEXT CH="  }"/>
            <para/>
            <para/>
            <ITEXT CH="  function Set_Row {"/>
            <para/>
            <ITEXT CH="    function Set_Row_Loop {"/>
            <para/>
            <ITEXT CH="      case &quot;$1&quot; in"/>
            <para/>
            <ITEXT CH="        &quot;0&quot;)"/>
            <para/>
            <ITEXT CH="          ;;"/>
            <para/>
            <ITEXT CH="        *)"/>
            <para/>
            <ITEXT CH="          echo &quot;$column_grid&quot; &gt;&gt; $local_file"/>
            <para/>
            <ITEXT CH="          Pred &quot;$1&quot;"/>
            <para/>
            <ITEXT CH="          Set_Row_Loop &quot;$RESULT&quot;"/>
            <para/>
            <ITEXT CH="          ;;"/>
            <para/>
            <ITEXT CH="      esac"/>
            <para/>
            <ITEXT CH="    }"/>
            <para/>
            <ITEXT CH="    Set_Row_Loop &quot;$1&quot;"/>
            <para/>
            <ITEXT CH="  }"/>
            <para/>
            <para/>
            <ITEXT CH="  SIZE_ROW=&quot;$1&quot;"/>
            <para/>
            <ITEXT CH="  SIZE_COLUMN=&quot;$2&quot;"/>
            <para/>
            <para/>
            <ITEXT CH="  Set_Column &quot;$2&quot;"/>
            <para/>
            <ITEXT CH="  Set_Row &quot;$1&quot;"/>
            <para/>
            <ITEXT CH="  mv $local_file $FILENAME"/>
            <para/>
            <ITEXT CH="}"/>
            <para/>
            <para/>
            <ITEXT CH="#-------------------------------#"/>
            <para/>
            <ITEXT CH="# move grid register            #"/>
            <para/>
            <ITEXT CH="#-------------------------------#"/>
            <para/>
            <ITEXT CH="function Move {"/>
            <para/>
            <ITEXT CH="  function Row {"/>
            <para/>
            <ITEXT CH="    ROW=$1"/>
            <para/>
            <ITEXT CH="  }"/>
            <para/>
            <para/>
            <ITEXT CH="  function Column {"/>
            <para/>
            <ITEXT CH="    COLUMN=$1"/>
            <para/>
            <ITEXT CH="  }"/>
            <para/>
            <para/>
            <ITEXT CH="  case $1 in"/>
            <para/>
            <ITEXT CH="    &quot;Row&quot; | &quot;Column&quot;)"/>
            <para/>
            <ITEXT CH="      $1 $2"/>
            <para/>
            <ITEXT CH="      ;;"/>
            <para/>
            <ITEXT CH="    *)"/>
            <para/>
            <ITEXT CH="      echo &quot;move $1 error&quot;"/>
            <para/>
            <ITEXT CH="      ;;"/>
            <para/>
            <ITEXT CH="  esac"/>
            <para/>
            <ITEXT CH="}"/>
            <para/>
            <para/>
            <ITEXT CH="#-------------------------------#"/>
            <para/>
            <ITEXT CH="# set grid value                #"/>
            <para/>
            <ITEXT CH="#-------------------------------#"/>
            <para/>
            <ITEXT CH="function Grid {"/>
            <para/>
            <ITEXT CH="  function Set {"/>
            <para/>
            <ITEXT CH="    local set_value=&quot;           $1&quot;"/>
            <para/>
            <ITEXT CH="    set_value=`echo &quot;$set_value&quot; | sed &quot;s/^.*\(.\{11\}\)$/\1/&quot;`"/>
            <para/>
            <para/>
            <ITEXT CH="    Pred &quot;$COLUMN&quot; &quot;1&quot;"/>
            <para/>
            <para/>
            <ITEXT CH="    case $RESULT in"/>
            <para/>
            <ITEXT CH="      &quot;0&quot;)"/>
            <para/>
            <ITEXT CH="        RESULT=0"/>
            <para/>
            <ITEXT CH="        ;;"/>
            <para/>
            <ITEXT CH="      *)"/>
            <para/>
            <ITEXT CH="        Mul &quot;11&quot; &quot;$RESULT&quot;"/>
            <para/>
            <ITEXT CH="        ;;"/>
            <para/>
            <ITEXT CH="    esac"/>
            <para/>
            <para/>
            <ITEXT CH="    search=&quot;\(.\{$RESULT\}\).\{11\}\(.*\)&quot;"/>
            <para/>
            <ITEXT CH="    row_re=$ROW&quot;s/$search/\1$set_value\2/&quot;"/>
            <para/>
            <ITEXT CH="    sed -i &quot;$row_re&quot; &quot;$FILENAME&quot;"/>
            <para/>
            <ITEXT CH="  }"/>
            <para/>
            <para/>
            <ITEXT CH="  case $1 in"/>
            <para/>
            <ITEXT CH="    &quot;Set&quot;)"/>
            <para/>
            <ITEXT CH="      $1 $2"/>
            <para/>
            <ITEXT CH="      ;;"/>
            <para/>
            <ITEXT CH="    *)"/>
            <para/>
            <ITEXT CH="      echo &quot;Grid set error&quot;"/>
            <para/>
            <ITEXT CH="      ;;"/>
            <para/>
            <ITEXT CH="  esac"/>
            <para/>
            <ITEXT CH="}"/>
            <para/>
            <para/>
            <ITEXT CH="#-------------------------------#"/>
            <para/>
            <ITEXT CH="# main                          #"/>
            <para/>
            <ITEXT CH="#-------------------------------#"/>
            <para/>
            <ITEXT CH="function Get_Command {"/>
            <para/>
            <ITEXT CH="  local command=`echo &quot;$1&quot; | sed &quot;s/\(.*\) .* .*/\1/&quot;`"/>
            <para/>
            <ITEXT CH="  local parameter=`echo &quot;$1&quot; | sed &quot;s/.* \(.*\) .*/\1/&quot;`"/>
            <para/>
            <ITEXT CH="  local target=`echo &quot;$1&quot; | sed &quot;s/.* .* \(.*\)/\1/&quot;`"/>
            <para/>
            <ITEXT CH="  $command $parameter $target"/>
            <para/>
            <ITEXT CH="}"/>
            <para/>
            <para/>
            <ITEXT CH="function Main_Loop {"/>
            <para/>
            <ITEXT CH="  local file_number=$2&quot;q;d&quot;"/>
            <para/>
            <ITEXT CH="  local new_line=`sed &quot;$file_number&quot; $1`"/>
            <para/>
            <ITEXT CH="  Get_Command &quot;$new_line&quot;"/>
            <para/>
            <ITEXT CH="  echo &quot;$new_line&quot;"/>
            <para/>
            <para/>
            <ITEXT CH="  if [ ! &quot;$2&quot; == &quot;$3&quot; ]"/>
            <para/>
            <ITEXT CH="  then"/>
            <para/>
            <ITEXT CH="    Add $2 &quot;1&quot;"/>
            <para/>
            <ITEXT CH="    Main_Loop &quot;$1&quot; &quot;$RESULT&quot; &quot;$3&quot;"/>
            <para/>
            <ITEXT CH="  fi"/>
            <para/>
            <ITEXT CH="}"/>
            <para/>
            <para/>
            <ITEXT CH="# FILENAME"/>
            <para/>
            <ITEXT CH="# SIZE_ROW"/>
            <para/>
            <ITEXT CH="# SIZE_COLUMN"/>
            <para/>
            <ITEXT CH="# ROW"/>
            <para/>
            <ITEXT CH="# COLUMN"/>
            <para/>
            <para/>
            <ITEXT CH="case $1 in"/>
            <para/>
            <ITEXT CH="  &quot;&quot;)"/>
            <para/>
            <ITEXT CH="    echo &quot;no data&quot;"/>
            <para/>
            <ITEXT CH="    ;;"/>
            <para/>
            <ITEXT CH="  *)"/>
            <para/>
            <ITEXT CH="    LAST_LINE=`grep -n &quot;.*&quot; $1 | tail -n 1 | sed &quot;s/\([1234567890]*\):.*/\1/&quot;`"/>
            <para/>
            <ITEXT CH="    SIZE_ROW=1"/>
            <para/>
            <ITEXT CH="    SIZE_COLUMN=1"/>
            <para/>
            <ITEXT CH="    ROW=1"/>
            <para/>
            <ITEXT CH="    COLUMN=1"/>
            <para/>
            <ITEXT CH="    Main_Loop $1 1 $LAST_LINE"/>
            <para/>
            <ITEXT CH="    ;;"/>
            <para/>
            <ITEXT CH="esac"/>
            <para/>
